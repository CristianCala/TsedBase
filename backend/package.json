{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend for atletas proyect",
  "scripts": {
    "test": "yarn run test:lint && yarn run test:coverage",
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --exit-child --cls --ignore node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "typeorm": "typeorm-ts-node-commonjs",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
  "dependencies": {
    "@tsed/ajv": "^7.18.2",
    "@tsed/cli-core": "4.4.5",
    "@tsed/common": "^7.18.2",
    "@tsed/core": "^7.18.2",
    "@tsed/di": "^7.18.2",
    "@tsed/engines": "^7.18.2",
    "@tsed/exceptions": "^7.18.2",
    "@tsed/json-mapper": "^7.18.2",
    "@tsed/logger": "^6.3.4",
    "@tsed/logger-file": "^6.3.4",
    "@tsed/openspec": "^7.18.2",
    "@tsed/passport": "^7.18.2",
    "@tsed/platform-cache": "^7.18.2",
    "@tsed/platform-exceptions": "^7.18.2",
    "@tsed/platform-express": "^7.18.2",
    "@tsed/platform-log-middleware": "^7.18.2",
    "@tsed/platform-middlewares": "^7.18.2",
    "@tsed/platform-params": "^7.18.2",
    "@tsed/platform-response-filter": "^7.18.2",
    "@tsed/platform-views": "^7.18.2",
    "@tsed/schema": "^7.18.2",
    "@tsed/swagger": "^7.18.2",
    "@tsed/typeorm": "^7.18.4",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nodemailer": "^6.4.7",
    "ajv": "^8.12.0",
    "barrelsby": "^2.5.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "method-override": "^3.0.0",
    "nodemailer": "^6.9.1",
    "pg": "^8.10.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^3.12.6",
    "twilio": "^4.11.1",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.4.5",
    "@tsed/cli-plugin-jest": "4.4.5",
    "@tsed/cli-plugin-passport": "4.4.5",
    "@tsed/cli-plugin-typeorm": "4.4.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^8.2.4",
    "@types/jest": "^29.5.0",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/passport": "^1.0.12",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "purgecss": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3"
  },
  "main": "app.js",
  "keywords": [
    "atletas",
    "proyect"
  ],
  "author": "ViloTechnologies",
  "license": "ISC",
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  }
}
